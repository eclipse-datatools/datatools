            id="org.eclipse.datatools.enablement.oracle.internal.ui.drivers.11.OracleThinDriverUIContributor">
      </driverUIContributor>
   </extension>
   
   <extension point="org.eclipse.datatools.connectivity.ui.driverUIContributor">
      <driverUIContributor
