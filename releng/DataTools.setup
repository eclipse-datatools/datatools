      <predicate
          xsi:type="predicates:AndPredicate">
        <operand
            xsi:type="predicates:RepositoryPredicate"
            project="org.eclipse.datatools.build"
            relativePathPattern="plugins/.*"/>
        <operand
            xsi:type="workingsets:ExclusionPredicate"
            excludedWorkingSet="//'dtp.workingsets'/@workingSets[name='DTP%20Docs']"/>
      </predicate>
    </workingSet>
    <workingSet
        name="DTP Docs">
      <predicate
          xsi:type="predicates:AndPredicate">
