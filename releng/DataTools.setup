        <operand
            xsi:type="predicates:RepositoryPredicate"
            project="org.eclipse.datatools.build"/>
        <operand
            xsi:type="workingsets:ExclusionPredicate"
            excludedWorkingSet="//'dtp.workingsets'/@workingSets[name='DTP%20Bundles'] //'dtp.workingsets'/@workingSets[name='DTP%20Features'] //'dtp.workingsets'/@workingSets[name='DTP%20Examples'] //'dtp.workingsets'/@workingSets[name='DTP%20Tests'] //'dtp.workingsets'/@workingSets[name='DTP%20Docs']"/>
